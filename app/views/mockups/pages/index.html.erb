<div class="min-h-screen bg-gradient-to-br from-gray-50 to-gray-100">
  <!-- Header -->
  <header class="bg-white shadow-sm border-b border-gray-200">
    <div class="max-w-7xl mx-auto px-6 py-6">
      <div class="flex items-center justify-between">
        <div class="flex items-center gap-4">
          <%= image_tag "logo_physiotraq_color.png", alt: "PhysioTraq", class: "h-12" %>
          <div>
            <h1 class="text-2xl font-bold text-gray-900">PhysioTraq Mockups</h1>
            <p class="text-sm text-gray-500">Design Preview & Navigation</p>
          </div>
        </div>
        <div class="flex items-center gap-2">
          <span class="inline-flex items-center px-3 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">
            Phase: MOCKUPS ✅ COMPLETE
          </span>
        </div>
      </div>
    </div>
  </header>

  <main class="max-w-7xl mx-auto px-6 py-12">
    <!-- Quick Stats -->
    <div class="grid grid-cols-3 gap-6 mb-12">
      <div class="bg-white rounded-xl shadow-sm p-6 border border-gray-100">
        <div class="flex items-center gap-4">
          <div class="w-12 h-12 rounded-lg bg-gradient-to-r from-[#D93611] to-[#F2811D] flex items-center justify-center">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 7a2 2 0 012 2m4 0a6 6 0 01-7.743 5.743L11 17H9v2H7v2H4a1 1 0 01-1-1v-2.586a1 1 0 01.293-.707l5.964-5.964A6 6 0 1121 9z"/>
            </svg>
          </div>
          <div>
            <p class="text-3xl font-bold text-gray-900">2</p>
            <p class="text-sm text-gray-500">Auth Pages</p>
          </div>
        </div>
      </div>
      <div class="bg-white rounded-xl shadow-sm p-6 border border-gray-100">
        <div class="flex items-center gap-4">
          <div class="w-12 h-12 rounded-lg bg-[#32D9D9] flex items-center justify-center">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z"/>
            </svg>
          </div>
          <div>
            <p class="text-3xl font-bold text-gray-900">6</p>
            <p class="text-sm text-gray-500">Admin Pages</p>
          </div>
        </div>
      </div>
      <div class="bg-white rounded-xl shadow-sm p-6 border border-gray-100">
        <div class="flex items-center gap-4">
          <div class="w-12 h-12 rounded-lg bg-[#008B8B] flex items-center justify-center">
            <svg class="w-6 h-6 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"/>
            </svg>
          </div>
          <div>
            <p class="text-3xl font-bold text-gray-900">5</p>
            <p class="text-sm text-gray-500">Operator Pages</p>
          </div>
        </div>
      </div>
    </div>

    <!-- Mockup Categories -->
    <div class="space-y-10">
      
      <!-- Auth Section -->
      <section>
        <div class="flex items-center gap-3 mb-6">
          <div class="w-10 h-10 rounded-lg bg-gradient-to-r from-[#D93611] to-[#F2811D] flex items-center justify-center">
            <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 7a2 2 0 012 2m4 0a6 6 0 01-7.743 5.743L11 17H9v2H7v2H4a1 1 0 01-1-1v-2.586a1 1 0 01.293-.707l5.964-5.964A6 6 0 1121 9z"/>
            </svg>
          </div>
          <div>
            <h2 class="text-xl font-bold text-gray-900">Authentication</h2>
            <p class="text-sm text-gray-500">Login & Registration flows</p>
          </div>
        </div>
        
        <div class="grid md:grid-cols-2 lg:grid-cols-4 gap-4">
          <%= link_to mockups_login_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#F2811D] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#FEF3E8] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#D93611]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M11 16l-4-4m0 0l4-4m-4 4h14m-5 4v1a3 3 0 01-3 3H6a3 3 0 01-3-3V7a3 3 0 013-3h7a3 3 0 013 3v1"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#D93611] transition-colors">Login</h3>
            <p class="text-sm text-gray-500 mt-1">User authentication page</p>
          <% end %>
          
          <%= link_to mockups_signup_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#F2811D] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#FEF3E8] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#D93611]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M18 9v3m0 0v3m0-3h3m-3 0h-3m-2-5a4 4 0 11-8 0 4 4 0 018 0zM3 20a6 6 0 0112 0v1H3v-1z"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#D93611] transition-colors">Sign Up</h3>
            <p class="text-sm text-gray-500 mt-1">New account registration</p>
          <% end %>
        </div>
      </section>

      <!-- Admin Section -->
      <section>
        <div class="flex items-center gap-3 mb-6">
          <div class="w-10 h-10 rounded-lg bg-[#32D9D9] flex items-center justify-center">
            <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m5.618-4.016A11.955 11.955 0 0112 2.944a11.955 11.955 0 01-8.618 3.04A12.02 12.02 0 003 9c0 5.591 3.824 10.29 9 11.622 5.176-1.332 9-6.03 9-11.622 0-1.042-.133-2.052-.382-3.016z"/>
            </svg>
          </div>
          <div>
            <h2 class="text-xl font-bold text-gray-900">Admin / License Owner</h2>
            <p class="text-sm text-gray-500">License management, devices, team & billing</p>
          </div>
        </div>
        
        <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-4">
          <%= link_to mockups_admin_dashboard_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#32D9D9] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8FAFA] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#32D9D9]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 5a1 1 0 011-1h14a1 1 0 011 1v2a1 1 0 01-1 1H5a1 1 0 01-1-1V5zM4 13a1 1 0 011-1h6a1 1 0 011 1v6a1 1 0 01-1 1H5a1 1 0 01-1-1v-6zM16 13a1 1 0 011-1h2a1 1 0 011 1v6a1 1 0 01-1 1h-2a1 1 0 01-1-1v-6z"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Dashboard</h3>
            <p class="text-sm text-gray-500 mt-1">Overview & statistics</p>
          <% end %>
          
          <%= link_to mockups_admin_devices_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#32D9D9] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8FAFA] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#32D9D9]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 3v2m6-2v2M9 19v2m6-2v2M5 9H3m2 6H3m18-6h-2m2 6h-2M7 19h10a2 2 0 002-2V7a2 2 0 00-2-2H7a2 2 0 00-2 2v10a2 2 0 002 2zM9 9h6v6H9V9z"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Devices</h3>
            <p class="text-sm text-gray-500 mt-1">Manage registered devices</p>
          <% end %>
          
          <%= link_to mockups_admin_device_detail_path(id: 1), class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#32D9D9] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8FAFA] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#32D9D9]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 5H7a2 2 0 00-2 2v12a2 2 0 002 2h10a2 2 0 002-2V7a2 2 0 00-2-2h-2M9 5a2 2 0 002 2h2a2 2 0 002-2M9 5a2 2 0 012-2h2a2 2 0 012 2m-3 7h3m-3 4h3m-6-4h.01M9 16h.01"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Device Detail</h3>
            <p class="text-sm text-gray-500 mt-1">Single device view & history</p>
          <% end %>
          
          <%= link_to mockups_admin_team_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#32D9D9] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8FAFA] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#32D9D9]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 4.354a4 4 0 110 5.292M15 21H3v-1a6 6 0 0112 0v1zm0 0h6v-1a6 6 0 00-9-5.197M13 7a4 4 0 11-8 0 4 4 0 018 0z"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Team</h3>
            <p class="text-sm text-gray-500 mt-1">Manage team members</p>
          <% end %>
          
          <%= link_to mockups_admin_billing_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#32D9D9] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8FAFA] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#32D9D9]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 10h18M7 15h1m4 0h1m-7 4h12a3 3 0 003-3V8a3 3 0 00-3-3H6a3 3 0 00-3 3v8a3 3 0 003 3z"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Billing</h3>
            <p class="text-sm text-gray-500 mt-1">Licenses & payments</p>
          <% end %>
          
          <%= link_to mockups_admin_treatments_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#32D9D9] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8FAFA] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#32D9D9]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 17v-2m3 2v-4m3 4v-6m2 10H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Treatments</h3>
            <p class="text-sm text-gray-500 mt-1">All treatments history</p>
          <% end %>
        </div>
      </section>

      <!-- Operator Section -->
      <section>
        <div class="flex items-center gap-3 mb-6">
          <div class="w-10 h-10 rounded-lg bg-[#008B8B] flex items-center justify-center">
            <svg class="w-5 h-5 text-white" fill="none" stroke="currentColor" viewBox="0 0 24 24">
              <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16 7a4 4 0 11-8 0 4 4 0 018 0zM12 14a7 7 0 00-7 7h14a7 7 0 00-7-7z"/>
            </svg>
          </div>
          <div>
            <h2 class="text-xl font-bold text-gray-900">Operator</h2>
            <p class="text-sm text-gray-500">Medical staff: monitoring & treatments</p>
          </div>
        </div>
        
        <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-4">
          <%= link_to mockups_operator_dashboard_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#008B8B] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8F5F5] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#008B8B]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 5a1 1 0 011-1h14a1 1 0 011 1v2a1 1 0 01-1 1H5a1 1 0 01-1-1V5zM4 13a1 1 0 011-1h6a1 1 0 011 1v6a1 1 0 01-1 1H5a1 1 0 01-1-1v-6zM16 13a1 1 0 011-1h2a1 1 0 011 1v6a1 1 0 01-1 1h-2a1 1 0 01-1-1v-6z"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Dashboard</h3>
            <p class="text-sm text-gray-500 mt-1">Overview & quick actions</p>
          <% end %>
          
          <%= link_to mockups_operator_monitor_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#008B8B] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8F5F5] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#008B8B]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 19v-6a2 2 0 00-2-2H5a2 2 0 00-2 2v6a2 2 0 002 2h2a2 2 0 002-2zm0 0V9a2 2 0 012-2h2a2 2 0 012 2v10m-6 0a2 2 0 002 2h2a2 2 0 002-2m0 0V5a2 2 0 012-2h2a2 2 0 012 2v14a2 2 0 01-2 2h-2a2 2 0 01-2-2z"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Live Monitor</h3>
            <p class="text-sm text-gray-500 mt-1">Real-time temperature display</p>
          <% end %>
          
          <%= link_to mockups_operator_new_treatment_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#008B8B] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8F5F5] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#008B8B]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 6v6m0 0v6m0-6h6m-6 0H6"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">New Treatment</h3>
            <p class="text-sm text-gray-500 mt-1">Start a new session</p>
          <% end %>
          
          <%= link_to mockups_operator_treatments_path, class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#008B8B] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8F5F5] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#008B8B]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M4 6h16M4 10h16M4 14h16M4 18h16"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Treatments</h3>
            <p class="text-sm text-gray-500 mt-1">My treatment history</p>
          <% end %>
          
          <%= link_to mockups_operator_treatment_detail_path(id: 1), class: "group bg-white rounded-xl shadow-sm border border-gray-100 p-6 hover:shadow-lg hover:border-[#008B8B] transition-all duration-200" do %>
            <div class="flex items-center justify-between mb-4">
              <div class="w-10 h-10 rounded-lg bg-gray-100 group-hover:bg-[#E8F5F5] flex items-center justify-center transition-colors">
                <svg class="w-5 h-5 text-gray-500 group-hover:text-[#008B8B]" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12h6m-6 4h6m2 5H7a2 2 0 01-2-2V5a2 2 0 012-2h5.586a1 1 0 01.707.293l5.414 5.414a1 1 0 01.293.707V19a2 2 0 01-2 2z"/>
                </svg>
              </div>
              <span class="inline-flex items-center px-2 py-1 rounded-full text-xs font-medium bg-green-100 text-green-800">DONE</span>
            </div>
            <h3 class="font-semibold text-gray-900 group-hover:text-[#008B8B] transition-colors">Treatment Detail</h3>
            <p class="text-sm text-gray-500 mt-1">View session data & export</p>
          <% end %>
        </div>
      </section>
    </div>

    <!-- Footer -->
    <footer class="mt-16 pt-8 border-t border-gray-200 text-center">
      <p class="text-sm text-gray-500">
        PhysioTraq Mockups • 
        <span class="text-[#32D9D9]">At the core of human care</span>
      </p>
    </footer>
  </main>
</div>
